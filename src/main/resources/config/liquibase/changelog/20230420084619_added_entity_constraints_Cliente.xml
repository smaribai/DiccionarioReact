<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Cliente.
    -->
    <changeSet id="20230420084619-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="tipo_cliente_codigo"
                                 baseTableName="cliente"
                                 constraintName="fk_cliente__tipo_cliente_id"
                                 referencedColumnNames="codigo"
                                 referencedTableName="tipo_cliente"/>

        <addForeignKeyConstraint baseColumnNames="pais_codigo_pais"
                                 baseTableName="cliente"
                                 constraintName="fk_cliente__pais_id"
                                 referencedColumnNames="codigo_pais"
                                 referencedTableName="pais"/>
    </changeSet>
</databaseChangeLog>
